  line-height: 1.563;
  color: #212529; }

/* Typography
––––––––––––––––––––––––––––––––– */
h1,
h2,
h3,
h4,
h5,
h6 {
  margin-bottom: 1rem;
  margin-top: 1em;
  font-weight: bold; }

h1 {
  font-size: 3.052rem;
  letter-spacing: -0.15rem;
  line-height: 1; }

