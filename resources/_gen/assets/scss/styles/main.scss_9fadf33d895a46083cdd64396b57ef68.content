  height: 3px;
  width: 25px;
  margin-top: 4px;
  margin-bottom: 4px;
  position: relative;
  left: 0;
  opacity: 1; }

#navbar-toggle .icon-bar:nth-child(1) {
  transform: rotate(0) translateY(0) translateX(0); }

#navbar-toggle .icon-bar:nth-child(2) {
  opacity: 1; }

#navbar-toggle .icon-bar:nth-child(3) {
  transform: rotate(0) translateY(0) translateX(0); }

#navbar-toggle.active .icon-bar:nth-child(1) {
  transform: rotate(-45deg) translateY(5px) translateX(-5px); }

#navbar-toggle.active .icon-bar:nth-child(2) {
  opacity: 0; }

#navbar-toggle.active .icon-bar:nth-child(3) {
  transform: rotate(45deg) translateY(-5px) translateX(-5px); }

body {
  display: flex;
  justify-content: center;
  align-items: center;
  min-height: 100vh;
  margin: 1rem; }

.container {
  max-width: 1200px;
  margin: 0 auto; }

.flex-grid {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between; }
  .flex-grid .col {
    flex-basis: 32%; }

.flex-grid-two {
  display: flex;
  justify-content: space-between; }
  .flex-grid-two .col {
    width: 49%; }

.flex-grid-three {
  display: flex;
  justify-content: space-between; }
  .flex-grid-three .col {
    width: 32.67%; }

.flex-grid-four {
  display: flex;
  justify-content: space-between; }
  .flex-grid-four .col {
    width: 24.5%; }
  .flex-grid-four img {
    display: block;
    margin: auto; }

@media (max-width: 768px) {
  .flex-grid, .flex-grid-two, .flex-grid-three, .flex-grid-four {
    display: block; }
    .flex-grid .col, .flex-grid-two .col, .flex-grid-three .col, .flex-grid-four .col {
      width: 100%;
      margin: 0 0 10px; } }

.justify-content-start {
  justify-content: flex-start; }

.align-items-start {
  align-items: start; }

.align-items-center {
  align-items: center; }

.align-items-end {
  align-items: end; }

